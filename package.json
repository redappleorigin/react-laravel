    "dev:server": "node node_modules\\webpack\\bin\\webpack.js --config client\\config\\server\\webpack.config.dev.js -d --watch",
    "setup:dev": "npm run setup:dev:sqlite && npm run setup:dev:env && npm run setup:dev:composer && npm run setup:dev:artisan && npm install",
    "setup:dev:sqlite": "bin\\php -r \"new SQLite3('database\\database.db');\"",
    "setup:dev:env": "echo APP_ENV=local > .env && echo APP_DEBUG=true >> .env && echo APP_URL=http://localhost >> .env && echo APP_KEY=SomeRandomString >> .env",
    "setup:dev:composer": "bin\\composer install",
    "setup:dev:artisan": "npm run setup:dev:artisan:key && npm run setup:dev:artisan:migrate",
    "setup:dev:artisan:key": "bin\\php artisan key:generate",
    "setup:dev:artisan:migrate": "bin\\php artisan migrate",
    "webpack:concurrently:dev": "concurrently --kill-others --raw \"npm run server:webpack:dev:build:watch\" \"npm run client:webpack:dev:build:watch\"",
    "server:webpack:dev:build:watch": "node node_modules\\webpack\\bin\\webpack.js --config webpack\\server.dev.js -d --watch",
