    "setup:dev:sqlite": "bin\\php -r \"new SQLite3('database\\database.db');\"",
    "setup:dev:env": "echo APP_ENV=local > .env && echo APP_DEBUG=true >> .env && echo APP_URL=http://localhost >> .env && echo APP_KEY=SomeRandomString >> .env",
    "setup:dev:composer": "bin\\composer install",
    "setup:dev:artisan": "npm run setup:dev:artisan:key && npm run setup:dev:artisan:migrate",
    "setup:dev:artisan:key": "bin\\php artisan key:generate",
    "setup:dev:artisan:migrate": "bin\\php artisan migrate",
    "webpack:server": "node node_modules\\webpack\\bin\\webpack.js --config webpack\\server.prod.js -d --watch",
    "webpack:client:dev": "node node_modules\\webpack\\bin\\webpack.js --config webpack\\client.dev.js -d --watch"
  },
